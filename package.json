{
  "name": "node-api",
  "version": "0.0.1",
  "license": "MIT",
  "description": "REST API with Express, MongoDB, JWT.",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "me-io/node-api",
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "ME.IO <info@me.io>",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "ini": "1.3.5",
    "morgan": "^1.9.0",
    "resource-router-middleware": "^0.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.4.0",
    "nodemon": "^1.18.4",
    "prettier": "1.14.2"
  }
}
